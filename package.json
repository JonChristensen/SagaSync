{
  "name": "saga-sync",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "infra"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "synth": "cd infra && npm run cdk:synth",
    "deploy": "cd infra && npm run cdk:deploy",
    "invoke:sample": "ts-node -r tsconfig-paths/register scripts/invokeSample.ts",
    "import:audible": "ts-node -r tsconfig-paths/register scripts/importAudible.ts",
    "audible:export": "python scripts/download_audible.py",
    "notion:reset": "ts-node -r tsconfig-paths/register scripts/notionReset.ts",
    "series:recompute": "ts-node -r tsconfig-paths/register scripts/recomputeSeries.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.5.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.902.0",
    "@aws-sdk/client-secrets-manager": "^3.901.0",
    "@aws-sdk/client-sfn": "^3.901.0",
    "@aws-sdk/lib-dynamodb": "^3.902.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.6.1",
    "undici": "^6.22.0"
  }
}
